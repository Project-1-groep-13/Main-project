        xmlns:local="clr-namespace:project_p1"
        mc:Ignorable="d"
        Title="HighScore" Height="600" Width="500">
    <Grid>
        <Label Content="High Score" FontFamily="algerian" FontSize="33" VerticalAlignment="Top" Margin="150,96,126,0" Opacity="2"/>
        <Button
                Content="Main Menu"
                Background="BurlyWood"
                FontSize="22"
                FontFamily="algerian" Margin="138,417,138,104" Click="Button_Click"/>

        <DataGrid
        Height="150"
        Width="450" 
        Margin="25,155,25,279"
        x:Name="dtGrid"
        ItemsSource="{Binding}">
         
        </DataGrid>
        <Button
            Content="enter data"
            Background="BurlyWood"
            FontSize="22"
            FontFamily="algerian" Margin="138,330,138,191" Click="Button_Click_1"/>


        <!--StackPanel 
            Height="250"
